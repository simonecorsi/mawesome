{
  "name": "mawesome",
  "version": "2.2.0",
  "description": "Generate awesome list from user starred repositories",
  "author": "Simone Corsi<simonecorsi.dev@gmail.com>",
  "license": "MIT",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prebuild": "rm -f index.js tsconfig.tsbuildinfo",
    "build": "tsup src/index.ts --format esm",
    "postbuild": "./node_modules/.bin/ncc build index.js -o ./",
    "style:lint": "eslint src --ext .ts",
    "style:prettier": "prettier \"src/**/*.ts\" --list-different --write",
    "dev": "tsup src/index.ts --watch --format esm",
    "prepare": "node prepare.js || echo 'Skipping prepare'"
  },
  "keywords": [
    "github",
    "action",
    "awesome",
    "list",
    "generator",
    "typescript",
    "javascript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@octokit/rest": "^19.0.4",
    "@octokit/types": "^6.41.0",
    "@saithodev/semantic-release-backmerge": "^2.1.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/ejs": "^3.1.1",
    "@types/got": "^9.6.12",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdown-toc": "^1.2.0",
    "nyc": "^15.0.1",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "ejs": "^3.1.8",
    "gh-star-fetch": "^1.5.0",
    "got": "^12.5.1",
    "remark": "^14.0.2",
    "remark-toc": "^8.0.1"
  },
  "volta": {
    "node": "18.9.1",
    "npm": "8.6.0"
  }
}
